{{#> popup/backdrop}}
{{#> cards/cards cardLink="./index.hbs" cardsClasses="sign-up-popup"}}

<h1 class="header">Регистрация</h1>

{{#>form/form formClasses="sign-up-popup__form" }}
<div>
  {{> input/input type="уьфшд" name="email" placeholder="Почта" }}
  {{> input/input type="text" name="login" placeholder="Логин" }}

  {{> input/input type="text" name="first_name" placeholder="Имя" }}
  {{> input/input type="text" name="second_name" placeholder="Фамилия" }}
  {{> input/input type="tel" name="phone" placeholder="Телефон" }}
  {{> input/input type="password" name="password" placeholder="Пароль" }}
  {{> input/input type="password" name="password_again" placeholder="Пароль (ещё раз)" }}
</div>
<div class="sign-up-popup__buttons-wrapper">
  {{> input/input inputClasses="button" type="submit" name="submit_sign_up" value="Sign up" }}
  <a href="./index.hbs">Sign in</a>
</div>
{{/form/form}}

{{/cards/cards}}
{{/popup/backdrop}}